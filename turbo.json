{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "ex:dev": {
      "cache": false,
      "dependsOn": [
        "build"
      ]
    },
    "ex:build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "build"
      ]
    }
  }
}
